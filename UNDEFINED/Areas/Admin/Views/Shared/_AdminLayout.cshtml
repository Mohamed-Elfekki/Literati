<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UNDEFINED | Dashboard</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="/Admin/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/Admin/dist/css/adminlte.min.css">

</head>

<body class="hold-transition sidebar-mini">


    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <strong><a href="/" class="nav-link">Home</a></strong>
                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto" style="display:flex; flex-direction:row; align-items:center">
                <li class="nav-items"><strong>UNDEFINED | Dashboard</strong></li>
                <li class="nav-item">
                    <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                        <i class="fas fa-expand-arrows-alt"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a class="brand-link">
               <span class="brand-text font-weight-bold">UNDEFINED | Dashboard</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">

                        <li class="nav-item menu-open">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-user-alt"></i>
                                <p>
                                    Users
                                    <i class="right fas fa-users"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a asp-action="Index" asp-controller="User" asp-area="Admin" class="nav-link active">
                                        <i class="far fa-users nav-icon"></i>
                                        <p>All Users</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a asp-action="Blocked" asp-controller="User" asp-area="Admin" class="nav-link">
                                        <i class="far fa-remove nav-icon"></i>
                                        <p>Blocked Users</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item menu-open">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>
                                    PUSH
                                    <i class="right fas fa-plus"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a asp-action="Create" asp-controller="Upload" asp-area="Admin" class="nav-link active">
                                        <i class="far fa-users nav-icon"></i>
                                        <p>PUSH Story</p>
                                    </a>
                                    <a asp-action="Private" asp-controller="Upload" asp-area="Admin" class="nav-link">
                                        <i class="far fa-users nav-icon"></i>
                                        <p>Private</p>
                                    </a>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item menu-open">
                            <a href="#" class="nav-link active">
                                <i class="nav-icon fas fa-phone-alt"></i>
                                <p>
                                    Contact Us
                                    <i class="right fas fa-plus"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a asp-action="Index" asp-controller="User" asp-area="Admin" class="nav-link active">
                                        <i class="far fa-users nav-icon"></i>
                                        <p>Contact Us</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>


        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @if (TempData["Success"] != null)
            {
                <div class="alert alert-success text-center">
                    <strong>@TempData["Success"]</strong>
                </div>
            }
            @if (TempData["Error"] != null)
            {
                <div class="alert alert-danger text-center">
                    <strong>@TempData["Error"]</strong>
                </div>
            }

            @RenderBody()

        </div>
    </div>


    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="/Admin/plugins/jquery/jquery.min.js"></script>
    <!--Moment Js-->
    <script src="/Admin/plugins/moment/moment.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/Admin/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Admin/dist/js/adminlte.min.js"></script>
    <script>
        $(document).ready(function () {
            $(".moment-date").each(function (index, item) {
                $(item).text(moment($(item).text()).fromNow());
            });
        });
    </script>
    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/signalr/dist/browser/signalr.min.js"></script>
    <script src="~/admin/app/notifications.js"></script>
</body>
</html>
